{
    "name": "flarum/release",
    "description": "Generates releases, contributors, issue reporters and donators",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Flarum\\Release\\": "src/"
        }
    },
    "require": {
        "php": ">= 8.1",
        "ext-intl": "*",
        "composer/semver": "^3.3",
        "gmostafa/php-graphql-client": "^1.13",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/container": "^9.22",
        "illuminate/support": "^9.22",
        "knplabs/github-api": "^3.7",
        "league/flysystem": "~3.0.16",
        "psr/cache": "~3.0",
        "symfony/console": "^6.1",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "symfony/var-dumper": "~6.0"
    },
    "config": {
        "sort-packages": true
    }
